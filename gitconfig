[push]
  default = current
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
	editor = /usr/bin/vim
[merge]
  ff = only
[fetch]
  prune = true
[rebase]
  autosquash = true
[user]
	email = alexcstark@gmail.com
	name = Alex Stark
[diff]
  tool = default-difftool
[difftool "default-difftool"]
  cmd = code --wait --diff $LOCAL $REMOTE
